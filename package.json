{
  "name": "tds-dito-vscode",
  "displayName": "TDS-Dito, your partner in AdvPL programming",
  "description": "Using artificial intelligence resources, TDS-Dito will assist you in AdvPL programming",
  "version": "0.0.2",
  "publisher": "TOTVS",
  "preview": true,
  "author": {
    "name": "TOTVS",
    "email": "tecnologia@totvs.com.br"
  },
  "license": "Apache-2.0",
  "keywords": [
    "advpl",
    "TLPP",
    "tds",
    "TOTVS",
    "protheus",
    "artificial intelligence",
    "code",
    "assistant",
    "ai",
    "development"
  ],
  "engines": {
    "vscode": "^1.85.1"
  },
  "icon": "icons/tds-dito.png",
  "categories": [
    "Machine Learning",
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/totvs/tds-dito-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/totvs/tds-dito-vscode.git"
  },
  "galleryBanner": {
    "color": "#100f11",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/github/stars/totvs/tds-dito-vscode?style=social",
      "description": "Star tds-dito-vscode on Github",
      "href": "https://github.com/totvs/tds-dito-vscode"
    },
    {
      "url": "https://img.shields.io/twitter/follow/totvs?style=social",
      "description": "Follow TOTVS on X",
      "href": "https://twitter.com/TOTVS"
    }
  ],
  "homepage": "https://github.com/totvs/tds-dito-vscode",
  "activationEvents": [
    "onLanguage:advpl"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "tds-dito-view",
          "name": "Dito: Chat",
          "visibility": "collapsed",
          "icon": "icons/tds-dito-24x24.png"
        }
      ]
    },
    "commands": [
      {
        "command": "tds-dito.afterInsert",
        "title": "After Insert",
        "category": "TDS-Dito"
      },
      {
        "command": "tds-dito.detail-health",
        "title": "Detail Health",
        "category": "TDS-Dito"
      },
      {
        "command": "tds-dito.login",
        "title": "Login",
        "category": "TDS-Dito"
      },
      {
        "command": "tds-dito.logout",
        "title": "Logout",
        "category": "TDS-Dito"
      },
      {
        "command": "tds-dito.attribution",
        "title": "Show Code Attribution",
        "category": "TDS-Dito"
      },
      {
        "command": "tds-dito.generateCode",
        "title": "Generate Code",
        "category": "TDS-Dito"
      },
      {
        "command": "tds-dito.explain",
        "title": "Explain Code",
        "category": "TDS-Dito"
      }
    ],
    "configuration": [
      {
        "title": "TDS-Dito",
        "properties": {
          "tds-dito.endPoint": {
            "order": 1,
            "type": "string",
            "default": "https://advpl.ds.dta.totvs.ai",
            "description": "endPoint"
          },
          "tds-dito.apiVersion": {
            "order": 2,
            "type": "string",
            "default": "v1",
            "description": "API Version"
          },
          "tds-dito.documentFilter": {
            "order": 3,
            "type": [
              "object",
              "array"
            ],
            "default": [
              {
                "language": "advpl"
              },
              {
                "language": "advpl-asp"
              }
            ],
            "description": "Filter documents to enable suggestions for"
          },
          "tds-dito.enableAutoSuggest": {
            "order": 4,
            "type": "boolean",
            "default": true,
            "description": "enableAutoSuggest"
          },
          "tds-dito.requestDelay": {
            "order": 5,
            "type": "number",
            "default": 600,
            "description": "Delay in typing to activate the IA"
          },
          "tds-dito.maxLine": {
            "order": 6,
            "type": "number",
            "default": 5,
            "description": "Maximum lines per suggestion"
          },
          "tds-dito.maxSuggestions": {
            "order": 7,
            "type": "number",
            "default": 3,
            "description": "Maximum suggestions per requisition"
          },
          "tds-dito.showBanner": {
            "type": "boolean",
            "default": true,
            "description": "Displays banner when starting"
          },
          "tds-dito.logLevel": {
            "type": "string",
            "enum": [
              "off",
              "debug",
              "verbose",
              "http",
              "info",
              "warn",
              "error"
            ],
            "default": "debug",
            "description": "Nível de severidade mínima a ser gravada no arquivo de ocorrências.",
            "enumDescriptions": [
              "Desligado (não recomendado)",
              "Depuração",
              "Detalhado",
              "Requisições HTTP",
              "Informações",
              "Avisos",
              "Erros"
            ]
          },
          "tds-dito.trace": {
            "type": "string",
            "enum": [
              "off",
              "trace",
              "debug",
              "info",
              "warning",
              "error"
            ],
            "default": "info",
            "description": "Nível de severidade mínima a ser apresentada no console da extensão.",
            "enumDescriptions": [
              "Desligado (não recomendado)",
              "Rastreamento",
              "Depuração",
              "Informações",
              "Avisos",
              "Erros"
            ]
          },
          "tds-dito.lastLogin": {
            "type": "string",
            "default": "",
            "description": "When last user logged",
            "deprecationMessage": "For internal use. Don´t edit please."
          },
          "tds-dito.tokensToClear": {
            "scope": "application",
            "type": "array",
            "default": [],
            "description": "tokensToClear",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.maxNewTokens": {
            "scope": "application",
            "type": "number",
            "default": 128,
            "description": "maxNewTokens",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.temperature": {
            "scope": "application",
            "type": "number",
            "default": 0.2,
            "description": "temperature",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.top_p": {
            "scope": "application",
            "type": "number",
            "default": 0.95,
            "description": "top_p",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.top_k": {
            "scope": "application",
            "type": "number",
            "default": 50,
            "description": "top_p",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.stop_sequence": {
            "scope": "application",
            "type": "array",
            "default": [],
            "description": "stop_sequence",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.fillInTheMiddle": {
            "scope": "application",
            "type": "number",
            "default": 1,
            "description": "fillInTheMiddle",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.contextWindow": {
            "scope": "application",
            "type": "number",
            "default": 0,
            "description": "contextWindow",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.tlsSkipVerifyInsecure": {
            "scope": "application",
            "type": "boolean",
            "default": false,
            "description": "tlsSkipVerifyInsecure",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          },
          "tds-dito.tokenizer": {
            "scope": "application",
            "type": "object",
            "default": {},
            "description": "tokenizer",
            "deprecationMessage": "Used by HF API. Extension version 0.0.1"
          }
        }
      }
    ],
    "keybindings": [
      {
        "key": "cmd+shift+l",
        "command": "editor.action.inlineSuggest.trigger"
      },
      {
        "key": "cmd+shift+a",
        "command": "tds-dito.attribution"
      },
      {
        "key": "ctrl+d ctrl+e",
        "command": "tds-dito.explain"
      }
    ]
  },
  "scripts": {
    "--BUILD--": "",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./src",
    "compile:views": "cd webview-ui && npm run compile",
    "--WATCH--": "",
    "watch": "npm-run-all watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "cd webview-ui && npm run watch",
    "lint": "eslint src --ext ts",
    "--PUBLISH--": "",
    "vscode:prepublish": "npm run compile",
    "--TEST--": "",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@huggingface/agents": "^0.0.5",
    "@huggingface/hub": "^0.13.0",
    "@huggingface/inference": "^2.6.4",
    "@vscode/l10n": "^0.0.18",
    "@vscode/l10n-dev": "^0.0.31",
    "fs-extra": "^11.2.0",
    "undici": "^6.4.0",
    "vscode-languageclient": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.11.5",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.3"
  },
  "extensionDependencies": [
    "TOTVS.tds-vscode"
  ]
}